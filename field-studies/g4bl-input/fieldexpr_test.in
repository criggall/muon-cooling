

### PHYSICS ###

physics QGSP_BIC doStochastics=0

### BEAM ###

beam gaussian particle=mu+ nEvents=1 \
    beamZ=0.0 beamX=0.0 beamY=0.0 beamXp=0.0 beamYp=0.0 \ 
    sigmaX=0.0 sigmaY=0.0 sigmaXp=0.0 sigmaYp=0.0 \ 
    meanMomentum=0.0 sigmaP=0.0 meanT=0.0 sigmaT=0.0

### REFERENCE PARTICLE ###

# Initial conditions:
param X=-11.2945 #mm
param Y=14.0917 #mm
param Xp=0.823201/224.995 #radians
param Yp=-1.27215/224.995 #radians

reference referenceMomentum=200 particle=mu+ \
    beamX=$X beamY=$Y beamZ=0.0 beamXp=$Xp beamYp=$Yp

#reference referenceMomentum=200 particle=neutron \
#    beamX=0.0 beamY=0.0 beamZ=0.0 beamXp=0.0 beamYp=0.0

trace nTrace=1 format=ascii oneNTuple=1

### FIELD EXPRESSION PARAMETERS ###

param period=4200

## Magnitude -- fit to sum of four sine terms:

param a=0.05489501
param k=0.00897611
param phi=1.57061852
param b=0.00804858
param c=-0.0024954
param d=-0.00184349
param offset=0.06083017

## Angle -- fit to sigmoid for regions between solenoids:

param aa=1.04734006
param bb=0.0211275114
param cc=-210.190509

### SOLENOIDS ###

param sol_current=-80.46

coil coil innerRadius=420 outerRadius=600 length=300 nSheets=10 maxR=3600 maxZ=6000 dR=15 dZ=15 tolerance=0.003 

solenoid SolPos coilName=coil current=-$sol_current color=0,1,1
solenoid SolNeg coilName=coil current=$sol_current color=0,1,0

### PLACEMENT ###

## Dipole field:

# constant-angle sections:
do i 0 6
    param angle=-pi/3*$i+2*pi/3
    #param angle=1.0139487771549618*$i
    fieldexpr field Bx=($a*sin($k*z+$phi)+$b*sin($k*2*z+$phi)+$c*sin($k*3*z+$phi)+$d*sin($k*4*z+$phi)+$offset)*cos($angle) \
        By=($a*sin($k*z+$phi)+$b*sin($k*2*z+$phi)+$c*sin($k*3*z+$phi)+$d*sin($k*4*z+$phi)+$offset)*sin($angle) \
        Bz=0 length=300 width=600 height=600
    place field x=0 y=0 z=$period/6*$i
enddo

# rotating sections:
do i 0 6
    param shift=$period/6*$i+350
    param angle=-($aa/(1+exp(-$bb*(z-$cc-350/2))))-pi/3*$i+2*pi/3
    #param angle=($aa/(1+exp(-$bb*(z-$cc))))+1.0139487771549618*$i
    fieldexpr field Bx=($a*sin($k*(z+350)+$phi)+$b*sin($k*2*(z+350)+$phi)+$c*sin($k*3*(z+350)+$phi)+$d*sin($k*4*(z+350)+$phi)+$offset)*cos($angle) \
        By=($a*sin($k*(z+350)+$phi)+$b*sin($k*2*(z+350)+$phi)+$c*sin($k*3*(z+350)+$phi)+$d*sin($k*4*(z+350)+$phi)+$offset)*sin($angle) \
        Bz=0 length=400 width=600 height=600
    place field x=0 y=0 z=$period/6*$i+350
enddo

