=require arguments on NTuple commands (1 sec)

=file test050.in 
param histoFile=""
physics QGSP doStochastics=0
beam ascii filename=test050.beam nEvents=9
virtualdetector format=BLTrackFile AF radius=200 length=1 color=1,1,1 \
	require='EventID != 2' file=test050.AF.txt
beamlossntuple loss format=ascii file=test050.bl.txt require='EventID != 2'
timentuple time format=ascii file=test050.time.txt time=0.4 \
	require='EventID != 2'
trace nTrace=99 format=ascii oneNTuple=1 file=test050.trace.txt \
	require='EventID != 2'
zntuple format=ascii file=test050.znt.txt z=200 require='EventID != 2'
place AF z=300
=file test050.beam 
