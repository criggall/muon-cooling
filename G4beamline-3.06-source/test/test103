=fieldexpr with front=1 (1 sec)

=file test103.in <<-!
param histoFile=""
physics QGSP_BERT doStochastics=0
beam ascii filename=test103.beam nEvents=9
fieldexpr F height=1000 width=1000 length=200 By=1
place F z=400 front=1
virtualdetector format=BLTrackFile AF radius=1000 length=1 file=test103.txt
place AF z=1000
=file test103.beam 
