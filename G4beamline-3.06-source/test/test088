=helicalharmonic (2 sec)

=platform WINDOWS
=omit need rm and tail on WINDOWS
=platform

=file test088.in 
param histoFile="" nEvents=500 totalCharge=1E13
physics QGSP_BERT
helicalharmonic hel radius=100 length=200 n=1 b=1 lambda=100
place hel z=0 
beam gaussian particle=mu- meanMomentum=200
printfield type=grid file="test088.full.txt" X0=-50 Y0=-50 nX=21 nY=21 \
	nZ=1 dX=5 dY=5 dZ=5
=run rm -f test088.full.txt
=file test088.good 
