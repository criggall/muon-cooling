=extrusion (2 sec)

=file test056.in 
param histoFile=""
physics QGSP doStochastics=0
beam ascii filename=test056.beam nEvents=25
extrusion E length=500  kill=1 color=1,0,0 \
	vertices=';50,5*10; 3.50*(50 + 50) ,50;  ; 350,350; 25*10,350; 250,250; 150,250; 100,350; 100/2,250;   ;   '
virtualdetector format=BLTrackFile AF radius=2000 length=1 \
	color=invisible,1,1 file=test056.txt
place E z=500
place AF z=1000
=file test056.beam 
