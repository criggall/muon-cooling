=tracing through FieldMaps and bend (1 sec)

=file test013.in 
param histoFile=""
physics QGSP doStochastics=0
beam ascii filename=test013.beam nEvents=9
fieldmap Field filename=test013.B
genericbend B fieldWidth=500 fieldHeight=500 fieldLength=500 \
	ironWidth=800 ironHeight=800 ironLength=500 \
	fringe=0 fieldMaterial=Vacuum
place Field z=1000 rename=F1
place B z=2000 rename=B1 By=-0.6500 rotation=Y15
corner C z=2000 rotation=Y30
place Field z=3000 rename=F2
trace format=asciiExtended oneNTuple=1 nTrace=2 filename=test013.AllTracks.txt
=file test013.B 
cylinder Z0=0 nR=2 nZ=2 dR=200 dZ=200
extendZ
data
  0   0 0.0 1.0
  0 200 0.0 1.0
200   0 0.0 1.0
200 200 0.0 1.0
=file test013.beam 
