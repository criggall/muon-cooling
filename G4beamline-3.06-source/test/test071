=basic collective tracking (2 sec)

=file test071.in 
param histoFile="" 
physics QGSP_BIC
collective deltaT=0.2 format=ascii filenamePrefix="test071."
trace oneNTuple=1 primaryOnly=1 nTrace=99999 format=ascii file=test071.AllTracks.txt
beam ascii file=test071.beam
virtualdetector Det height=500 width=500 length=1 format=ascii file=test071.txt
place Det z=100
=file test071.beam 
