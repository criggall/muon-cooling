=boolean command, geometry test (2 sec)
# check boolean subtraction, volume subtracted does not intersect other volumes

=file test110.in 
param histoFile="" 
physics FTFP_BERT doStochastics=0
beam ascii file=beam
extrusion E length=100  \
	vertices=';50,5*10; 3.50*(50 + 50) ,50;  ; 350,350; 25*10,350; 250,250; 150,250; 100,350; 100/2,250;   ;   '
cylinder s2 outerRadius=40 length=300
cylinder s3 outerRadius=80 length=300
boolean op=subtraction s5 E s2 x=125 y=125 kill=1 color=1,0,0
boolean op=subtraction s6 s5 s3 x=350 y=50 kill=1 color=1,0,0
place s6 z=75
box B height=100 width=100 length=100 
box B2 height=100 width=100 length=101
boolean s9 op=subtraction B B2 x=-50 y=50 kill=1 color=0,0,1
place s9 z=75 x=355 y=45
virtualdetector Det height=900 width=900 length=100 format=ascii file=test110.txt
place Det z=200
=testbeam beam 0 400 11 0 400 11 0 -13 200
=file test110.good 
