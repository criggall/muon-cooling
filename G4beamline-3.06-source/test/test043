					coordinates=global
place D2 z=282.843 x=282.843 coordinates=global rotation=Y45
box end length=1 height=200 width=200
place end z=500
beamlossntuple LossCenterline format=ascii filename=test043.lossCL.txt
beamlossntuple LossGlobal format=ascii filename=test043.lossG.txt \
					coordinates=global
timentuple TimeCL time=1.0 format=ascii filename=test043.timeCL.txt
timentuple TimeGlobal time=1.0 format=ascii filename=test043.timeG.txt \
					coordinates=global
trace nTrace=7 oneNTuple=1 format=ascii filename=test043.AllTracks.txt \
					coordinates=global
printf "%.3f %.3f %.3f" x y z z=200 file=test043.printf.txt \
					coordinates=global
