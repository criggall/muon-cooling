=ntuple union (2 sec)

=platform WINDOWS
=omit unknown error in WINDOWS
=platform

=file test090.in 
physics QGSP doStochastics=0
beam ascii filename=test090.beam nEvents=9
virtualdetector Det format=BLTrackFile radius=2000 length=1 color=1,1,1
particlefilter Killer height=1000 width=100 kill=mu+
particlefilter Keeper height=1000 width=1000 keep=mu+
place Keeper z=500
place Det z=1000 rename=test090.Det#
place Det z=2000 rename=test090.Det#
place Det z=3000 rename=test090.Det#
place Det z=4000 rename=test090.Det#
place Killer z=4500 x=200
place Killer z=4500 x=-200
place Det z=5000 rename=test090.Det#
ntuple test090.All format=ascii detectors=test090.Det* union=1
=file test090.beam 
