# comment surrounded by blank lines

=platform WINDOWS
=run dir
=exists dir.out
=run copy dir.out zap
=exists zap
=compare dir.out zap

=platform LINUX 

=platform DARWIN

=platform LINUX DARWIN
=run ls
=exists ls.out
=run cp ls.out zap
=exists zap
=compare ls.out zap
=run diff ls.out zap

=platform
=ignore Unknown test command '=fail'
=fail
=ignore failed
=run noSuchProgram

=file A
# comment . . .
1 2 3 X 5
2 4 6 Y 8
1 3 5 7 9
=file B
# comment . . .
1 2 3 X 5
2 4 6 Y 8
1 3 5 7 9
=compare A B
=compare A B .1 .1 .1 -1 .1
=file B1
# comment . . .
1 2 3 X 5
2 4 6 Y 8
1 3 5 7 8 a b c
=ignore Cannot read file 'C1'
=compare A C1 .1 .1 .1 -1 .1
=ignore Files 'A' and 'B1' differ at line 4
=compare A B1 
=ignore Files 'A' and 'B1' differ at line 4, column 5
=compare A B1 .1 .1 .1 -1 .1
=compare A B1 .1 .1 .1 -1 1.1
=compare A B1 .1 .1 .1 
=file B2
1 2 3 X 5
2 4 6 Y 8
1 3 5 7 8
=ignore Files 'A' and 'B2' differ at line 1
=compare A B2
=ignore Files 'A' and 'B2' comment mismatch, line 1
=compare A B2 .1 .1 .1
=file B3
# different comment . . .
1 2 3 X 5
2 4 6 Y 8
=ignore Files 'A' and 'B3' differ at line 1
=compare A B3
=ignore Files 'A' and 'B3' have different lengths
=compare A B3 .1 .1 .1 -1 .1
=file B4
# different comment . . .
1 2 3 X 5
2 4 6 Y
1 3 5 7 8
=ignore File 'A' and/or 'B4' has too few columns, line 3
=compare A B4 .1 .1 .1 -1 .1
=ignore File 'A' and/or 'B4' has invalid data on line 2, column 4
=compare A B4 .1 .1 .1 .1
=file B5
# comment . . .
1,2,3,X,5
2, 4, 6, Y, 8
1 , 3 , 5 , 7 , 9
=compare A B5 .1 .1 .1 -1 .1
=file B6
2, 4, 6, Y, 8
1 , 3 , 5 , 7 , 9
=fileContainedIn B6 B5
=filelines B5 > 3
=filelines B5 = 4
=filelines B5 < 5
=exists B6
