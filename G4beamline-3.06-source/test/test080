=particlesource command (2 sec)

=file test080.in 
param histoFile=""
physics QGSP doStochastics=0
particlesource nEvents=10
/gps/particle proton
/gps/pos/type Point
/gps/pos/centre 0 0 0 mm
/gps/ang/type iso
/gps/ang/mintheta 0 deg
/gps/ang/maxtheta 0.001 deg
/gps/energy 2. MeV
/gps/ang/rot1 -1 0 0
virtualdetector Det radius=1000 length=1 format=ascii file=test080.txt
place Det z=1000
=file test080.good 
